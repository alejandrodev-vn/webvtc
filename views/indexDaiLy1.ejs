<!DOCTYPE html>
<html>
    <head>
        <title>
            <%= title %>
        </title>
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <%- include('header') %> 
        <link rel="stylesheet" href="../stylesheets/organization.css">
        <link rel="stylesheet" href="../stylesheets/adminhome.css">
    </head>

    <body>
        <div class="wrapper">
            <div class="box">
                <%- include('menuDaily')  %> 
            </div>
            <div class="container">
                <div class="row">
                  <div class="nm-fm col-md-12">
                    <h2 class="title">TẠO YÊU CẦU CẤP MỚI CHỨNG THƯ SỐ</h2>
                    <div class="choose-certificate">
                        <a href="/digital-certificate/personal" class="btn btn-primary">Cá nhân</a>
                        <a href="/digital-certificate/organization"  class="btn btn-primary">Tổ chức</a>
                    </div>
                </div>
                <!-- edit -->
                <div id="modalEditCertificatePersonal" class="modal">

                    <!-- Modal content -->
                    <div class="modal-content">
                        <div class="form-area">
                              <div class="top-modal">
                                <h2 style="margin-bottom:15px; color:firebrick;" class="title-modal">Chỉnh sửa yêu cầu CTS Cá nhân</h2>
                                <span class="close btn-cancel">&times;</span>
                            </div>
                              <form action="/digital-certificate/personal/edit" method="POST" enctype="multipart/form-data">
                                <div id="form">
                                    <div class="info-user">
                                        <div>
                                            <h3>Thông tin khách hàng</h3>
                                        </div>
                                        <div id="wrt-info">
                                            <div class="if-fm mn" >
                                                <label>
                                                    Họ và tên <span class="note">*</span>
                                                </label>
                                                <input type="text" id="hoTenNguoiDK" name="hoTenNguoiDK">
                                            </div>
                                            <div class="if-fm">
                                                <div class="if-fm ck">
                                                    <label>Số CMTND/ Hộ Chiếu <span class="note">*</span></label>
                                                    <input type="text" id="soCMT" name="soCMT">
                                                </div>
                                                <div class="if-fm ck">
                                                    <label>Ngày cấp <span class="note">*</span></label>
                                                    <input type="date" id="ngayCapCMT" name="ngayCapCMT">
                                                </div>
                                            </div>
                                            <div class="if-fm">
                                                <div class="if-fm ck">
                                                    <label>Nơi cấp <span class="note">*</span></label>
                                                    <input type="text" id="noiCapCMT" name="noiCapCMT">
                                                </div>
                                                <div class="if-fm ck">
                                                    <label>Mã số thuế</label>
                                                    <input type="text" id="MSTCaNhan" name="MSTCaNhan">
                                                </div>
                                            </div>
                                            <div class="if-fm mn">
                                                <label>
                                                    Địa chỉ  <span class="note">*</span>
                                                </label>
                                                <input type="text" id="diaChi" name="diaChi">
                                            </div>
                                            <div class="if-fm">
                                                <div class="if-fm ck">
                                                    <label>Mã số thuế công ty</label>
                                                    <input type="text" id="MSTCongTy" name="MSTCongTy">
                                                </div>
                                                <div class="if-fm ck">
                                                    <label>Ngành nghề</label>
                                                    <input type="text" id="nganhNghe" name="nganhNghe">
                                                </div>
                                            </div>
                                            <div class="if-fm mn">
                                                <label>
                                                    Công ty/ Tổ chức
                                                </label>
                                                <input type="text" id="tenCongTy" name="tenCongTy">
                                            </div>
                                            <div class="if-fm mn">
                                                <label>
                                                    Chức vụ
                                                </label>
                                                <input type="text" id="chucVu" name="chucVu">
                                            </div>
                                            <div class="if-fm">
                                                <div class="if-fm ck">
                                                    <label>Tỉnh/ Thành Phố <span class="note">*</span></label>
                                                    <select name="tinhThanh" id="tinhThanh" class="tinhThanh" onchange="getQuanHuyen(this.value)">
                                                        <option value="0">--Lựa chọn--</option>
                                                    </select>
                                                </div>
                                                <div class="if-fm ck">
                                                    <label>Quận/ Huyện <span class="note">*</span></label>
                                                    <select name="quanHuyen" id="quanHuyen">
                                                        <option value="0">Chọn tỉnh thành trước</option>
                                                    </select>
                                                </div>
                                            </div>
                                            <div class="if-fm">
                                                <div class="if-fm ck">
                                                    <label>Email cá nhân  <span class="note">*</span></label>
                                                    <input type="text" id="email" name="email">
                                                </div>
                                                <div class="if-fm ck">
                                                    <label>Số điện thoại  <span class="note">*</span></label>
                                                    <input type="text" id="soDienThoai" name="soDienThoai">
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                    
                                    <div class="info-user">
                                        <div>
                                            <h3>Yêu cầu cấp chứng thư số</h3>
                                        </div>
                                        <div id="wrt-info">
                                            <div class="if-fm">
                                                <div class="if-fm ck">
                                                    <label>Gói chứng thư <span class="note">*</span></label>
                                                    <select name="goiCTSId" id="services" onchange="chooseService(this.value)">
                                                
                                                    </select>
                                                </div>
                                                <div class="if-fm ck">
                                                    <label>Thời hạn sử dụng <span class="note">*</span> </label>
                                                    <input type="text"  name="thoiHan" id="thoiHan" readonly="true" style="background:#eee">
                                                </div>
                                            </div>
                                            <div class="if-fm">
                                                <div class="if-fm ck">
                                                    <label>Giá cước</label>
                                                    <input type="text" name="gia" id="gia" readonly="true" style="background:#eee">
                                                </div>
                                                <div class="if-fm ck">
                                                    <label>Người thực hiện <span class="note">*</span></label>
                                                    <input type="text" name="nguoiThucHien" readonly="true" value="<%= locals.user.hoTen %>" style="background:#eee">
                                                </div>
                                                <input type="hidden" name="createdBy" value="<%= locals.user._id %>">
                                            </div>
                                        </div>
                                        <% if(typeof errors != 'undefined') { 
            
                                            errors.forEach(msg => {    
                                        %> 
                                        <div class="alert alert-warning alert-dismissible fade show" role="alert">
                                            <%= msg.msg %>
                                            <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                                              <span aria-hidden="true">&times;</span>
                                            </button>
                                        </div>
                                   
                                        <% }) }%>
                                    </div>
                                </div>
            
                                <div class="hoso">
                                    <div>
                                        <h3>Hồ sơ khách hàng</h3>
                                    </div>
                                    <div id="documentPersonal"></div>

                                    <div id="wrt-info">
                                        <div class="if-fm mn" >
                                            <label>
                                                Chọn tệp: 
                                            </label>
                                            <input type="file" name="fileHoSo" />
            
                                        </div>
                                    </div>
                                </div>
                     
                                <div id="register">
                                    <div class="rg-btn">
                                        <input type="submit" value="Ghi lại" class="btn btn-primary">
                                        <input type="button" value="Đóng" class="btn btn-secondary btn-cancel">
                                        <input type="hidden" name="idEdit" id="id">
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
                <!-- end-modal personal -->
                <!-- edit -->
                <div id="modalEditCertificateOrganization" class="modal">
                        <!-- Modal content -->
                    <div class="modal-content">
                        <div class="form-area">
                              <div class="top-modal">
                                <h2 style="margin-bottom:15px; color:firebrick;" class="title-modal">Chỉnh sửa yêu cầu CTS Doanh Nghiệp</h2>
                                <span class="close btn-cancelDN">&times;</span>
                              </div>
